{
  "name": "contributte/recaptcha",
  "description": "Google reCAPTCHA for Nette - Forms",
  "keywords": [
    "nette",
    "forms",
    "captcha",
    "recaptcha",
    "google"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/reCAPTCHA",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "suggest": {
    "ext-openssl": "To make requests via https"
  },
  "require": {
    "php": ">=8.1",
    "nette/di": "^3.1.8",
    "nette/forms": "^3.1.14",
    "nette/utils": "^4.0.3"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1" ,
    "mockery/mockery": "^1.6.7"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\ReCaptcha\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.1.x-dev"
    }
  }
}
